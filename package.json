		"@wordpress/e2e-test-utils": "^7.9.0",
		"@wordpress/edit-post": "^6.10.0",
		"@wordpress/element": "^4.7.0",
		"@wordpress/env": "^4.9.0",
		"@wordpress/eslint-plugin": "^12.7.0",
