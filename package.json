		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@wordpress/api-fetch": "^6.10.0",
		"@wordpress/babel-preset-default": "^6.11.0",
		"@wordpress/block-editor": "^9.2.0",
		"@wordpress/blocks": "^11.12.0",
		"@wordpress/components": "^19.11.0",
